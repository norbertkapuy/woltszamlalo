{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/expenses.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/expenses/create.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/expenses/[id].js",
      "use": "@vercel/node"
    },
    {
      "src": "api/summary.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/months.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/expenses",
      "dest": "/api/expenses.js"
    },
    {
      "src": "/api/expenses/create",
      "dest": "/api/expenses/create.js"
    },
    {
      "src": "/api/expenses/(.*)",
      "dest": "/api/expenses/[id].js"
    },
    {
      "src": "/api/summary",
      "dest": "/api/summary.js"
    },
    {
      "src": "/api/months",
      "dest": "/api/months.js"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.js"
    }
  ]
}